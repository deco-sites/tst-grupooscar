{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s",
    "--btn-text-case": "normal-case"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "600"
          },
          {
            "weight": "700"
          }
        ],
        "family": "Plus Jakarta Sans"
      }
    ]
  },
  "mode": "dark",
  "fonts": [
    {
      "fontFamily": "Poppins"
    }
  ],
  "mainColors": {
    "neutral": "#f8f8f8",
    "primary": "#9D0B0D",
    "warning": "#e08e00",
    "base-100": "#ffffff",
    "tertiary": "#25D366",
    "secondary": "#E4002B",
    "info": "#1f1f1f"
  },
  "complementaryColors": {
    "primary": {
      "primary-content": "#ffffff"
    },
    "base-200": "#D9D9D9",
    "base-300": "#F5f5f5",
    "base-content": "#4B4C4D",
    "neutral-content": "#f9f9f9",
    "primary-content": "#000000",
    "secondary-content": "#fffbc2",
    "tertiary-content": "#242424"
  }
}